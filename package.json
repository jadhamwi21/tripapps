{
  "name": "tripapps-cli",
  "version": "1.6.1",
  "description": "cli tool to manage tripapps, and scrape/populate data",
  "author": "Jad",
  "bin": {
    "tripapps-cli": "./bin/run"
  },
  "homepage": "https://github.com/TripApps",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "TripApps/hello-world",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "app-store-scraper": "^0.17.0",
    "cli-table": "^0.3.11",
    "cli-table3": "^0.6.3",
    "console-table-printer": "^2.11.2",
    "files": "^2.2.2",
    "google-play-scraper": "^9.1.1",
    "inquirer": "^8.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^7.3.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.28",
    "@types/chai": "^4",
    "@types/cli-table": "^0.3.1",
    "@types/dotenv-flow": "^3.3.1",
    "@types/inquirer": "^9.0.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.38",
    "chai": "^4",
    "dotenv-flow": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "tripapps-cli",
    "dirname": "tripapps-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": ":",
    "topics": {}
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "tarballs": "oclif pack tarballs",
    "deb": "oclif pack deb"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/TripApps/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
